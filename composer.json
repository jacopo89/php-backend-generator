{
    "description": "This bundle helps define a resource model of entities",
    "homepage": "",
    "keywords": ["generator", "code generator", "scaffolding", "scaffold"],
    "authors": [
        {
            "name": "Symfony Community",
            "homepage": "https://symfony.com/contributors"
        }
    ],
    "type": "symfony-bundle",
    "name": "jacopo89/php-backend-generator",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "api-platform/core": "^2.5",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "docusign/esign-client": "^5.7",
        "gaufrette/extras": "^0.1.0",
        "gesdinet/jwt-refresh-token-bundle": "^0.9.1",
        "knplabs/doctrine-behaviors": "^2.0",
        "knplabs/knp-gaufrette-bundle": "^0.7.1",
        "league/csv": "^9.7",
        "lexik/jwt-authentication-bundle": "^2.9",
        "nelmio/cors-bundle": "^2.1",
        "phpdocumentor/reflection-docblock": "^5.2",
        "sensio/framework-extra-bundle": "^6.1",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "5.1.*",
        "symfony/console": "5.1.*",
        "symfony/debug-bundle": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/expression-language": "5.1.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.1.*",
        "symfony/intl": "5.1.*",
        "symfony/maker-bundle": "^1.21",
        "symfony/messenger": "5.1.*",
        "symfony/mime": "5.1.*",
        "symfony/monolog-bundle": "^3.6",
        "symfony/property-access": "5.1.*",
        "symfony/property-info": "5.1.*",
        "symfony/security-bundle": "5.1.*",
        "symfony/serializer": "5.1.*",
        "symfony/stopwatch": "^5.1",
        "symfony/twig-bundle": "^5.1",
        "symfony/validator": "5.1.*",
        "symfony/var-dumper": "5.1.*",
        "symfony/web-profiler-bundle": "^5.1",
        "symfony/workflow": "5.1.*",
        "symfony/yaml": "5.1.*"
    },
    "require-dev": {
        "justinrainbow/json-schema": "^5.2",
        "symfony/browser-kit": "5.2.*",
        "symfony/css-selector": "^5.2",
        "symfony/http-client": "5.2.*",
        "symfony/phpunit-bridge": "^5.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "BackendGenerator\\Bundle\\BackendGeneratorBundle": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BackendGenerator\\Bundle\\BackendGeneratorBundle\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    }
}
