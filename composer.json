{
    "description": "This bundle helps define a resource model of entities",
    "keywords": ["generator", "code generator", "scaffolding", "scaffold"],
    "authors": [
        {
            "name": "Symfony Community",
            "homepage": "https://symfony.com/contributors"
        }
    ],
    "type": "symfony-bundle",
    "name": "jacopo89/php-backend-generator",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "^2.6",
        "composer/package-versions-deprecated": "1.11.99.1",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.8",
        "gaufrette/extras": "^0.1.0",
        "gesdinet/jwt-refresh-token-bundle": "^0.11.1",
        "knplabs/doctrine-behaviors": "^2.2",
        "knplabs/knp-gaufrette-bundle": "^0.7.1",
        "lexik/jwt-authentication-bundle": "^2.11",
        "nelmio/cors-bundle": "^2.1",
        "phpdocumentor/reflection-docblock": "^5.2",
        "sensio/framework-extra-bundle": "^6.1",
        "symfony/asset": "5.2.*",
        "symfony/console": "5.2.*",
        "symfony/debug-bundle": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/expression-language": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.2.*",
        "symfony/maker-bundle": "^1.30",
        "symfony/messenger": "5.2.*",
        "symfony/monolog-bundle": "^3.7",
        "symfony/property-access": "5.2.*",
        "symfony/property-info": "5.2.*",
        "symfony/proxy-manager-bridge": "5.2.*",
        "symfony/security-bundle": "5.2.*",
        "symfony/serializer": "5.2.*",
        "symfony/twig-bundle": "5.2.*",
        "symfony/validator": "5.2.*",
        "symfony/web-profiler-bundle": "5.2.*",
        "symfony/workflow": "5.2.*",
        "symfony/yaml": "5.2.*"
    },
    "require-dev": {
        "justinrainbow/json-schema": "^5.2",
        "symfony/browser-kit": "5.2.*",
        "symfony/css-selector": "^5.2",
        "symfony/http-client": "5.2.*",
        "symfony/phpunit-bridge": "^5.2"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "BackendGenerator\\Bundle\\BackendGeneratorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BackendGenerator\\Bundle\\BackendGeneratorBundle\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
